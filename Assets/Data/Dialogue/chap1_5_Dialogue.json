  {
    "diaID": 0,
    "index": 20,
    "speaker": "Lily",
    "face": "Normal",
    "dialogueText": "…네!"
  },
  {
    "diaID": 1,
    "index": 0,
    "speaker": "Lily",
    "face": "Sad",
    "dialogueText": "마법이 풀렸나요?"
